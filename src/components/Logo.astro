---
interface Props {
  style: string;
  loading?: 'eager' | 'lazy' | null | undefined;
}

import { cn } from '@/lib/utils';
import { Image } from 'astro:assets';
import santienzPng from '../../public/santienz.png';

const { style, loading } = Astro.props;
---

<Image
  src={santienzPng}
  alt="Santienz Logo"
  class={cn('drop-shadow-lg', style)}
  loading={loading}
/>
